{
  "version": 2,
  "builds": [
    {
      "src": "api/graphql.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/graphql",
      "dest": "/api/graphql"
    },
    {
      "src": "/(.*)",
      "dest": "/api/graphql"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

